{
  "name": "Interactive-timeline",
  "version": "1.0.0",
  "description": "Client for Interactive Timeline RDS",
  "scripts": {
    "start": "node server.js",
    "test": "cross-env NODE_ENV=test mocha --recursive --compilers js:babel-register --require ./test/setup.js",
    "test:watch": "npm test -- --watch",
    "webpack-dev": "cross-env NODE_ENV=development node --trace-warnings -r babel-register ./node_modules/.bin/webpack-dev-server --config webpack.config.js",
    "watch": "START_HOT=$START_HOT npm run webpack-dev"
  },
